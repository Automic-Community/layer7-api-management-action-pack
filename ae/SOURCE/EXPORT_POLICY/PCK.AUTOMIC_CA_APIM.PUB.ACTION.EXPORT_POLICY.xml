<SCRIPT mode="1" state="1">
<MSCRI><![CDATA[!Setting Filename and path for exported policy
: IF &UC4RB_APIM_EXPORT_PATH# NE ""
:    SET &AGENT_TEMPDIR# = &UC4RB_APIM_EXPORT_PATH#
: ELSE
:    SET &AGENT_TEMPDIR# = GET_VAR('UC_EX_PATH_TEMP', &OVERWRITE_RA_AGENT#)
: ENDIF

: PSET &UC4RB_CA_APIM_POLICY_PATH# = STR_CAT(&AGENT_TEMPDIR#,"policy-&UC4RB_APIM_POLICY_ID#-&$RUNID#.xml")

!Check for Include Only Service Policy and Include Only Dependencies
:SET &COUNT# = 0
:IF &UC4RB_APIM_ONLY_SERVICE_POLICY# EQ "Yes"
:  SET &COUNT# = &COUNT# + 1
:ENDIF
:IF &UC4RB_APIM_ONLY_DEPENDENCIES# EQ "Yes"
:  SET &COUNT# = &COUNT# + 1
:ENDIF
:IF &COUNT# EQ 2
:       STOP MSG, 50, "Please provide true value either for Include Only Service Policy or Include Only Dependencies"
:ENDIF

! Setting all query parameters value
:SET &query_value_export_type# = &UC4RB_APIM_EXPORT_TYPE#

:SET &ATTRIBUTE_VARA# = "PCK.CA_APIM.PRV.VARA.MAP_BY"
:SET &UC4RB_MAP_BY_ATTRIBUTE# = &UC4RB_APIM_MAP_BY#
:INC PCK.CA_APIM.PUB.INCLUDE.ITERATE_VARA
:SET &query_value_map_by# = &ATTRIBUTE#


:IF &UC4RB_APIM_INC_REQ_FOLDER# EQ "Yes"
:  SET &query_value_inc_req_folder# = "true"
:ELSE
:    SET &query_value_inc_req_folder# = "false"
:ENDIF

:IF &UC4RB_APIM_EXPORT_GATEWAY_RMS# EQ "Yes"
:  SET &query_value_export_gateway_rms# = "true"
:ELSE
:    SET &query_value_export_gateway_rms# = "false"
:ENDIF

:IF &UC4RB_APIM_INC_DEPENDENCIES# EQ "Yes"
:  SET &query_value_inc_dependencies# = "true"
:ELSE
:    SET &query_value_inc_dependencies# = "false"
:ENDIF

:IF &UC4RB_APIM_ENCRYPT_SECRETS# EQ "Yes"
:  SET &query_value_encrypt_secrets# = "true"
:ELSE
:    SET &query_value_encrypt_secrets# = "false"
:ENDIF

:IF &UC4RB_APIM_ENC_CLUSTER# EQ "Yes"
:  SET &query_value_enc_cluster# = "true"
:ELSE
:    SET &query_value_enc_cluster# = "false"
:ENDIF

:IF &UC4RB_APIM_ONLY_SERVICE_POLICY# EQ "Yes"
:  SET &query_value_only_srvc_policy# = "true"
:ELSE
:    SET &query_value_only_srvc_policy# = "false"
:ENDIF

:IF &UC4RB_APIM_ONLY_DEPENDENCIES# EQ "Yes"
:  SET &query_value_only_dependencies# = "true"
:ELSE
:    SET &query_value_only_dependencies# = "false"
:ENDIF

! Setting all header parameters value

:IF &UC4RB_APIM_L7_KEY_PASSPHRASE# NE "--10"
:  SET &header_value_L7_key_passphrase# = &UC4RB_APIM_L7_KEY_PASSPHRASE#
:ELSE
:  SET &header_value_L7_key_passphrase# = ""
:ENDIF]]></MSCRI>
